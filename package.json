{
  "name": "ngx-auth-firebaseui",
  "description": "Open Source Library for Angular Web Apps to integrate a material user interface for firebase authentication",
  "version": "3.4.1",
  "homepage": "https://ngx-auth-firebaseui.firebaseapp.com",
  "author": {
    "name": "anthonynahas",
    "url": "https://github.com/anthonynahas"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/anthonynahas/ngx-auth-firebaseui.git"
  },
  "license": "MIT",
  "schematics": "./schematics/collection.json",
  "keywords": [
    "ng",
    "library",
    "angular",
    "@angular/fire",
    "firebase",
    "firebaseui",
    "firebase auth",
    "authentication",
    "firebaseauthentication",
    "firestore",
    "material",
    "material design",
    "ssr"
  ],
  "bugs": {
    "url": "https://github.com/anthonynahas/ngx-auth-firebaseui/issues"
  },
  "private": true,
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp build:watch",
    "build:watch:fast": "gulp build:watch-fast",
    "build:demo": "gulp build:demo",
    "build:doc": "gulp build:doc",
    "build:schematics": "gulp build:schematics",
    "es5": "babel dist -d dist --presets @babel/preset-env",
    "serve:demo": "gulp serve:demo",
    "serve:demo:ssr": "gulp serve:demo-ssr",
    "serve:doc": "gulp serve:doc",
    "link": "gulp link",
    "unlink": "gulp unlink",
    "test": " gulp test",
    "test:schematics": "npm run build && jasmine src/**/*_spec.js",
    "test:watch": " gulp test:watch",
    "demo": " gulp build:demo",
    "extract": "ng xi18n --output-path=locale",
    "check-travis": " travis-status --repo=anthonynahas/ngx-auth-firebaseui --branch=master --fail-pending || echo 'Not yet passing'"
  },
